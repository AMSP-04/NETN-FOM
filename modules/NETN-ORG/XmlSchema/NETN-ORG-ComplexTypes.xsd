	## Changes made for AMSP-04 Ed A
		* Updated BoundarySymbolModifiersType
			* Added StatusPresent
		* Added HoldingsType
		* Added UnitTypeType (jc3idem)
		* Updated UnitModelType
			* Added EntityType
		* Updated EquipmentItemType
			* Added NSN_Code
			* Added NSN_Name
			* Added Post_Type_Category
			* Added Post_Type_Rank
			* Added Holdings
		* Updated EquipmentModelType
			* Added EntityType
		* Updated EquipmentRelationsType
			* Added SupportRelations
		* Renamed InstallationType element
			* patternInstallationSymbolID15 to patternInstallationSymbolAPP6B
		* Renamed METOCGraphicTypeelement
			* patternMETOCSymbolID15 to patternMETOCSymbolAPP6B
		* Renamed MOOTWGraphicType
			* patternMOOTWSymbolID15 to patternMOOTWSymbolAPP6B
		* Renamed TacticalGraphicType
			* patternTacticalGraphicSymbolID15 to patternTacticalGraphicSymbolAPP6B
		* Updated MilitaryScenarioType
			* Added Deployment
		* Updated UnitType
			* Added Type
			* Added Holdings
			* Renamed patternForceSymbolID15 to patternForceSymbolAPP6B

	## Additional Changes made for AMSP-04 Ed B
		* Changed schema attribute elementFormDefault= from qualified to unqualified
		* Updated MilitaryScenarioType.Deployment
  			* Name – No Length restrictions
  			* Units – minOccurs = 0
  			* Unit – type
		* Changed type of all SymbolIdentifier elements to patternSymbolId and updated documentation.
		* Added  minOccurs="0" to datatypes for Unit and Equipment deployment elements.
		* Changed complex type EquipmentItemType to allow additional elements. Changed from <all> to <choice maxOccurs="unbounded"> and added CustomData element.
		* Changed complex type UnitType to allow additional elements. Changed from <all> to <choice maxOccurs="unbounded"> and added CustomData element.
		* targetNamespace changed to urn:nato:nmsg:ms3:netn:org:1
		
-->
<xs:schema xmlns:msdl="urn:nato:nmsg:ms3:netn:org:1" 
	xmlns:modelID="http://www.sisostds.org/schemas/modelID"
	xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.1:oo:2.0" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="urn:nato:nmsg:ms3:netn:org:1" 
	elementFormDefault="unqualified" 
	attributeFormDefault="unqualified" 
	version="1.0.0">
	
	<xs:include schemaLocation="NETN-ORG-Codes.xsd"/>
	<xs:include schemaLocation="NETN-ORG-SimpleTypes.xsd"/>
	<xs:import namespace="http://www.sisostds.org/schemas/modelID" schemaLocation="extern/ModelID_v2006_FINAL.xsd"/>
	<xs:import namespace="urn:int:nato:standard:mip:jc3iedm:3.1:oo:2.0" schemaLocation="extern/JC3IEDMMeteorological.xsd"/>
	<xs:complexType name="AnchorPointType">
		<xs:annotation>
			<xs:documentation>Single anchor point description.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="AnchorChoice" type="msdl:enumAnchorPointType">
				<xs:annotation>
					<xs:documentation>The type of anchor as a coordinate/location or point graphic.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Anchor" type="msdl:AnchorType">
				<xs:annotation>
					<xs:documentation>Specific anchor, as a point graphic or a location coordinate.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="AnchorPointsType">
		<xs:annotation>
			<xs:documentation>Anchor points of the symbol as established.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="AnchorPoint" type="msdl:AnchorPointType">
				<xs:annotation>
					<xs:documentation>Single anchor point description.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AnchorType">
		<xs:annotation>
			<xs:documentation>Specific anchor, as a point graphic or a location coordinate.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="Location" type="msdl:CoordinatesType">
				<xs:annotation>
					<xs:documentation>The coordinates of an object or anchor point.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PointSymbolHandle" type="msdl:patternUUIDRef32">
				<xs:annotation>
					<xs:documentation>UUID of the Point Symbol whose location is to be used as an anchor point.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="AreaSymbolModifiersType">
		<xs:annotation>
			<xs:documentation>Modifiers of an Area Symbol.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Echelon" type="msdl:enumEchelon" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Graphic modifier that identifies Command level.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalInfo" type="msdl:text20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The text modifier for an additional information field.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalInfo1" type="msdl:text20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The text modifier for an additional information field.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalInfo2" type="msdl:text20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The text modifier for an additional information field.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UniqueDesignation" type="msdl:text21" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The text modifier for the symbol's unique designation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateTimeGroup" type="msdl:patternTimeDTGRelative20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Date Time Group an Alphanumeric field for date/time which identifies the valid timeframe start, relative to ScenarioTime, for which the symbol is valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateTimeGroup1" type="msdl:patternTimeDTGRelative20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Date Time Group an Alphanumeric field for date/time which identifies the valid timeframe end, relative to ScenarioTime, for which the symbol is valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="AssociatedOverlaysType">
		<xs:annotation>
			<xs:documentation>The overlays for which this tactical graphic is to be displayed.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="OverlayHandles" type="msdl:OverlayHandlesType">
				<xs:annotation>
					<xs:documentation>References to all overlays on which this graphic is to be included.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceOverlayType" type="msdl:enumOverlayType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The overlay type of the original overlay on which the tactical graphic originated.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="AssociationsType">
		<xs:annotation>
			<xs:documentation>Associations between forces and sides.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="Association" type="msdl:AssociationType">
				<xs:annotation>
					<xs:documentation>Association between this force/side and another force/side.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AssociationType">
		<xs:annotation>
			<xs:documentation>Association between this force/side and another force/side.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="AffiliateHandle" type="msdl:patternUUIDRef32">
				<xs:annotation>
					<xs:documentation>Reference to another Force or Side which has some affiliation to this force.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Relationship" type="jc3iedm:ObjectItemHostilityStatusCode">
				<xs:annotation>
					<xs:documentation>A force or side's relationship with another force or side determines the level of threat posed by that force or side.  Units or entities inherit their affiliation to other units or entities based on the relationships between their containing forces and sides.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="BoundarySymbolModifiersType">
		<xs:annotation>
			<xs:documentation>Modifiers of a Boundary Symbol.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Echelon" type="msdl:enumEchelon" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Graphic modifier that identifies Command level.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusPresent" type="msdl:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identify if boundary status is 'Present', or Anticipated/Planned.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UniqueDesignation" type="msdl:text21" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The text modifier for the symbol's unique designation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UniqueDesignation1" type="msdl:text21" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Second designation used for lines and boundaries.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CloudCoverItemsType">
		<xs:annotation>
			<xs:documentation>JC3IEDM cloud cover conditions of the environment.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="CloudCover" type="jc3iedm:CloudCover">
				<xs:annotation>
					<xs:documentation>Collection of JC3IEDM cloud cover conditions of the environment.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommandRelationType">
		<xs:annotation>
			<xs:documentation>The command relationship between this unit/equipment and its commanding unit.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="CommandingSuperiorHandle" type="msdl:patternUUIDRef32">
				<xs:annotation>
					<xs:documentation>Unique identifier of the unit commanding this unit.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommandRelationshipType" type="msdl:enumCommandRelationshipType">
				<xs:annotation>
					<xs:documentation>Type of command under which the unit has been task organized as taken from FM 100-7 'The Army In Theater Operations'.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CommunicationNetInstancesType">
		<xs:annotation>
			<xs:documentation>The communication nets that the unit defines for communication with other units during a mission.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="CommunicationNetInstance" type="msdl:CommunicationNetInstanceType">
				<xs:annotation>
					<xs:documentation>An instance of a Communication net defined for a unit.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommunicationNetInstanceType">
		<xs:annotation>
			<xs:documentation>An instance of a Communication net defined for a unit.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="CommunicationNetType" type="msdl:enumCommunicationNetType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The tactical use/purpose of the communication network.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommunicationNetId" type="msdl:textIdentifier64">
				<xs:annotation>
					<xs:documentation>The Name or ID of the Communication net.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommunicationService" type="msdl:enumCommunicationServiceType">
				<xs:annotation>
					<xs:documentation>The type of service being used for communication as defined in JC3IEDM.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CommunicationNetReferencesType">
		<xs:annotation>
			<xs:documentation>The communication nets that equipment uses/subscribes to in order to communicate with other items of equipment.</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="CommunicationNetReference" type="msdl:CommunicationNetReferenceType">
				<xs:annotation>
					<xs:documentation>The reference to a CommunicationNetInstance that equipment monitors or uses to communicate with a mission.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CommunicationNetReferenceType">
		<xs:annotation>
			<xs:documentation>The reference to a CommunicationNetInstance that equipment monitors or uses to communicate with a mission.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CommunicationNetId" type="msdl:textIdentifier64">
				<xs:annotation>
					<xs:documentation>The Name or ID of the Communication net.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnitOwnerHandle" type="msdl:patternUUIDRef32">
				<xs:annotation>
					<xs:documentation>UUID reference to the Unit that owns a particular item of data as part of that unit's organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CoordinateDataStandardType">
		<xs:annotation>
			<xs:documentation>Denotes the coordinate system and datum used in the specification of the scenario.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="CoordinateSystemType" type="msdl:enumCoordinateSystemType">
				<xs:annotation>
					<xs:documentation>Coordinate system used in this scenario.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CoordinateSystemDatum" type="msdl:textDatum8">
				<xs:annotation>
					<xs:documentation>The Datum used to translate/calculate coordinates for the scenario.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CoordinatesType">
		<xs:annotation>
			<xs:documentation>A data structure containing location coordinates.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="CoordinateChoice" type="msdl:enumCoordinateSystemType">
				<xs:annotation>
					<xs:documentation>The enumerated choice of the type of coordinate (point) used to represent the location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CoordinateData" type="msdl:CoordinatePointType">
				<xs:annotation>
					<xs:documentation>The choice of coordinate data (point) used to represent the location.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CoordinatePointType">
		<xs:choice>
			<xs:element name="MGRS" type="msdl:MGRSType">
				<xs:annotation>
					<xs:documentation>MGRS coordinate.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UTM" type="msdl:UTMType">
				<xs:annotation>
					<xs:documentation>Universal Transverse Mercator Coordinate.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GDC" type="msdl:GDCType">
				<xs:annotation>
					<xs:documentation>Geodetic coordinates in decimal degrees of latitude and longitude.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GCC" type="msdl:GCCType">
				<xs:annotation>
					<xs:documentation>Geocentric Coordinate.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="EnvironmentType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The Environment for the Scenario.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="ScenarioTime" type="msdl:patternTimeDTG20" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The initial scenario date and time in the ISO 8601:2000 DateTime Group format yyyy-mm-ddThh:mm:ssZ e.g. 1998-05-12T14:15:00Z.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AreaOfInterest" type="msdl:RectangleAreaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The area of concern to all commanders in the scenario relative to the objectives of current or planned operations, including the areas of influence, operations and/or responsibility, and areas adjacent thereto.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScenarioWeather" type="msdl:ScenarioWeatherType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specification of the scenario weather conditions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="METOC" type="msdl:METOCType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Tactical Symbols representing the Meteorological conditions for the scenario.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="EquipmentDispositionType">
		<xs:annotation>
			<xs:documentation>Disposition of the equipment.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Location" type="msdl:CoordinatesType">
				<xs:annotation>
					<xs:documentation>The coordinates of an object or anchor point.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DirectionOfMovement" type="msdl:floatCompassDegrees3_3" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Direction of Movement, in compass degrees, of a scenario element.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Speed" type="msdl:floatSpeed6_2" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The field speed in meters per second.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FormationPosition" type="msdl:FormationPositionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The position of this unit/equipment in the higher unit formation.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="EquipmentItemType">
		<xs:annotation>
			<xs:documentation>Information about a single piece of equipment.</xs:documentation>
		</xs:annotation>
	
		<xs:all>
			<xs:element name="ObjectHandle" type="msdl:patternUUID32" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The Universal Unique Identifier (UUID) of an object.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SymbolIdentifier" type="msdl:patternSymbolId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A symbol identifier is represented as a string. 
The symbol standard used is indicated using an optional uri notation (uri:xxxxxxxxxx). 
The following uri should be used for common symbology standards app6b, app6b, app6c, app6c, 2525b, 2525c, 2525d. 
If not provided the default symbol standard used is the one specified in the ScenarioDataStandards element of the XML file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NSN_Code" type="msdl:patternNSN" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The 13 character of NATO Stock Number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NSN_Name" type="msdl:textName255" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The human readable name of the NNS code.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="msdl:textName255" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unabbreviated designation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EquipmentSymbolModifiers" type="msdl:EquipmentSymbolModifiersType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Modifiers of an Equipment Symbol.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommunicationNetReferences" type="msdl:CommunicationNetReferencesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The communication nets that equipment uses/subscribes to in order to communicate with other items of equipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Disposition" type="msdl:EquipmentDispositionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Disposition consists of the location of units and the manner in which these units are tactically deployed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Relations" type="msdl:EquipmentRelationsType" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Relations identify the realtionship of equipment in terms of owner and organic relationships.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Model" type="msdl:EquipmentModelType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Model specifies any detail impacting on the simulation model of the equipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Post_Type_Category" type="jc3iedm:MilitaryPostTypeCategoryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The military category post type.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Post_Type_Rank" type="jc3iedm:MilitaryPostTypeRankCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The military rank post type.</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="Holdings" type="msdl:HoldingsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The quantity of all specific Equipment/Consumable that is held by, installed in, or included with the unit.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomData" type="xs:anyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Any additional information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
		
	</xs:complexType>
	<xs:complexType name="EquipmentModelType">
		<xs:annotation>
			<xs:documentation>Model specifies any detail impacting on the simulation model of the equipment</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="Resolution" type="msdl:enumModelResolutionType">
				<xs:annotation>
					<xs:documentation>Enumeration indicating the level of fidelity appropriate for instancing the unit or equipment in the simulation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EntityType" type="msdl:patternEntityType"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="EquipmentRelationsType">
		<xs:annotation>
			<xs:documentation>Relations identify owner and organic relationships for an item of equipment.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="OrganicSuperiorHandle" type="msdl:patternUUIDRef32" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reference to the unit's higher organic organization.</xs:documentation>
